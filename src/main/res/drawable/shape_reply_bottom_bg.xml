<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    
    <!-- 圆角 -->
    <corners
        android:radius="5dp"
         
       /><!-- 设置圆角半径 -->
    <!-- 描边 -->
    <stroke
        android:width="0.5dp"
        android:color="#E4E4E1" /> <!-- 定义描边的宽度和描边的颜色值 -->
        
    <size
        android:width="70dp"
        android:height="50dp"/>
    <!-- 填充 -->
    <solid
        android:color="@color/white"/><!-- 填充的颜色 -->
    
 
</shape>